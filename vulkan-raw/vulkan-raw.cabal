library

    build-tool-depends:  hsc2hs:hsc2hs
    hs-source-dirs:      src
                       , src-gen

    build-depends:
        base >= 4.9 && < 5
      , storable-offset

    default-language:    Haskell2010
    ghc-options:         -Wall -Wno-unused-imports -Wno-missing-pattern-synonym-signatures

    if flag (stdcall)
      cpp-options:       -DCALLCV=stdcall
    elif flag (ccall)
      cpp-options:       -DCALLCV=ccall
    else
      cpp-options:       -DCALLCV=capi

    reexported-modules:  Foreign.Storable.Offset

    extra-libraries:     vulkan

    if flag (platform-android)
      cpp-options: -DVK_USE_PLATFORM_ANDROID_KHR

    if flag (platform-directfb)
      cpp-options: -DVK_USE_PLATFORM_DIRECTFB_EXT

    if flag (platform-fuchsia)
      cpp-options: -DVK_USE_PLATFORM_FUCHSIA

    if flag (platform-ggp)
      cpp-options: -DVK_USE_PLATFORM_GGP

    if flag (platform-ios)
      cpp-options: -DVK_USE_PLATFORM_IOS_MVK

    if flag (platform-macos)
      cpp-options: -DVK_USE_PLATFORM_MACOS_MVK

    if flag (platform-metal)
      cpp-options: -DVK_USE_PLATFORM_METAL_EXT

    if flag (beta)
      cpp-options: -DVK_ENABLE_BETA_EXTENSIONS

    if flag (platform-screen)
      cpp-options: -DVK_USE_PLATFORM_SCREEN_QNX

    if flag (platform-vi)
      cpp-options: -DVK_USE_PLATFORM_VI_NN

    if flag (platform-wayland)
      cpp-options: -DVK_USE_PLATFORM_WAYLAND_KHR

    if flag (platform-win32)
      cpp-options: -DVK_USE_PLATFORM_WIN32_KHR

    if flag (platform-xcb)
      cpp-options: -DVK_USE_PLATFORM_XCB_KHR

    if flag (platform-xlib)
      cpp-options: -DVK_USE_PLATFORM_XLIB_KHR

    if flag (platform-xlib-xrandr)
      cpp-options: -DVK_USE_PLATFORM_XLIB_XRANDR_EXT

    if flag (ffi-core-1-1)
      cpp-options: -DCORE_1_1

    if flag (ffi-core-1-2)
      cpp-options: -DCORE_1_2

