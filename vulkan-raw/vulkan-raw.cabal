cabal-version:       2.2

name:                vulkan-raw
version:             0.1.0.0
synopsis:            Raw bindings to the Vulkan API
description:         Please check the README for information on how these bindings operate.
license:             BSD-3-Clause
license-file:        LICENSE
author:              Oleksii Divak
maintainer:          Oleksii Divak <frozenwitness@gmail.com>
copyright:           Copyright: (c) 2023 Oleksii Divak
category:            Graphics
build-type:          Simple

flag ccall
  description: Use the ccall call convention.
  default: False

flag stdcall
  description: Use the stdcall call convention (takes precedence over the ccall flag).
  default: False

flag core-1-1
  description: Provide foreign function imports for core functions up to Vulkan 1.1.
  default: False

flag core-1-2
  description: Provide foreign function imports for core functions up to Vulkan 1.2.
  default: False

flag core-1-3
  description: Provide foreign function imports for core functions up to Vulkan 1.3.
  default: False

flag platform-android
  description: Enable extensions protected by VK_USE_PLATFORM_ANDROID_KHR
  default: False

flag platform-directfb
  description: Enable extensions protected by VK_USE_PLATFORM_DIRECTFB_EXT
  default: False

flag platform-fuchsia
  description: Enable extensions protected by VK_USE_PLATFORM_FUCHSIA
  default: False

flag platform-ggp
  description: Enable extensions protected by VK_USE_PLATFORM_GGP
  default: False

flag platform-ios
  description: Enable extensions protected by VK_USE_PLATFORM_IOS_MVK
  default: False

flag platform-macos
  description: Enable extensions protected by VK_USE_PLATFORM_MACOS_MVK
  default: False

flag platform-metal
  description: Enable extensions protected by VK_USE_PLATFORM_METAL_EXT
  default: False

flag beta
  description: Enable extensions protected by VK_ENABLE_BETA_EXTENSIONS
  default: False

flag platform-screen
  description: Enable extensions protected by VK_USE_PLATFORM_SCREEN_QNX
  default: False

flag platform-vi
  description: Enable extensions protected by VK_USE_PLATFORM_VI_NN
  default: False

flag platform-wayland
  description: Enable extensions protected by VK_USE_PLATFORM_WAYLAND_KHR
  default: False

flag platform-win32
  description: Enable extensions protected by VK_USE_PLATFORM_WIN32_KHR
  default: False

flag platform-xcb
  description: Enable extensions protected by VK_USE_PLATFORM_XCB_KHR
  default: False

flag platform-xlib
  description: Enable extensions protected by VK_USE_PLATFORM_XLIB_KHR
  default: False

flag platform-xlib-xrandr
  description: Enable extensions protected by VK_USE_PLATFORM_XLIB_XRANDR_EXT
  default: False

library

    build-tool-depends:  hsc2hs:hsc2hs
    hs-source-dirs:      src
                       , src-gen

    build-depends:
        base >= 4.9 && < 5
      , storable-offset

    default-language:    Haskell2010
    ghc-options:         -Wall -Wno-unused-imports -Wno-missing-pattern-synonym-signatures

    if flag (stdcall)
      cpp-options:       -DCALLCV=stdcall
    elif flag (ccall)
      cpp-options:       -DCALLCV=ccall
    else
      cpp-options:       -DCALLCV=capi

    if flag (core-1-1)
      cpp-options:       -DCORE_1_1

    if flag (core-1-2)
      cpp-options:       -DCORE_1_1 -DCORE_1_2

    if flag (core-1-3)
      cpp-options:       -DCORE_1_1 -DCORE_1_2 -DCORE_1_3

    reexported-modules:  Foreign.Storable.Offset

    pkgconfig-depends:   vulkan

    exposed-modules:
